{
  "name": "qqi",
  "type": "module",
  "version": "0.1.1",
  "description": "简单的文件读写",
  "license": "MIT",
  "scripts": {
    "b": "rollup --config rollup.config.js && tsc -p tsconfig.types.json",
    "clean:package-json": "node ./scripts/clean-package-json.js",
    "build": "jja rm dist && pnpm run b && pnpm run clean:package-json",
    "diff": "jja pkg --diff",
    "eslint": "jja cls && eslint src",
    "beautify": "pnpm run prettier",
    "prettier": "jja cls && prettier . --write",
    "push": "gvv",
    "push:version": "gvv",
    "vjj": "vjj -d",
    "dev": "rollup --config rollup.config.eg.js && node .eg/index.mjs"
  },
  "dependencies": {
    "@color-pen/static": "^1.0.1",
    "@qqi/log": "^0.1.1",
    "a-node-tools": "^4.2.19",
    "a-type-of-js": "^1.0.7",
    "color-pen": "^2.0.13"
  }
}
